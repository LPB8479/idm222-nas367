<!DOCTYPE html>
<html lang="en">
<!-- @import "global/head.kit" -->

<body>
    <!-- @import "global/header.kit" -->

    <main class="content">
        <section class="title">
            <h1>Archive</h1>
        </section>
        <section class="gallery" id="archive">
            <picture>
                <source
                        media="(min-width: 1440px)"
                        srcset="/assets/claystudio-xl.png" />
                <img src="assets/claystudio-min.png" alt="The Clay Studio Logo">
            </picture>
            <picture>
                <source
                        media="(min-width: 1440px)"
                        srcset="/assets/flamegif-xl.gif" />
                <img src="assets/flamegif-min.gif" alt="Flame Animation">
            </picture>
            <picture>
                <source
                        media="(min-width: 1440px)"
                        srcset="/assets/fontposter-xl.png" />
                <img src="assets/fontposter-min.png" alt="Freight Sans Poster">
            </picture>
            <picture>
                <source
                        media="(min-width: 1440px)"
                        srcset="/assets/indexed-min.png" />
                <img src="assets/indexed-min.png" alt="Indexed cover">
            </picture>
            <picture>
                <source
                        media="(min-width: 1440px)"
                        srcset="/assets/ensemblearts-xl.png" />
                <img src="assets/ensemblearts-min.png" alt="Ensemble Arts">
            </picture>
            <picture>
                <source
                        media="(min-width: 1440px)"
                        srcset="/assets/walters-xl.png" />
                <img src="assets/walters-min.png" alt="Walters Box">
            </picture>
        </section>
        <!-- @import "global/footer.kit" -->

    </main>
</body>
<!-- @import "global/scripts.kit"-->

</html>